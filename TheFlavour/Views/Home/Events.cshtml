        <script>
            jQuery(document).ready(function() {
                function testimonialsInit() {
                    jQuery("#testimonials861").carouFredSel({
                        swipe : {
                            onTouch: true
                        },
                        next : "#testimonials-next861",
                        prev : "#testimonials-prev861",
                        pagination : "#testimonials-controls861",
                        infinite: false,
                        items: 1,
                        auto: {
                            play: true,
                            timeoutDuration: 10000
                        },
                        scroll: {
                            items : 1,
                            fx: "crossfade",
                            easing: "linear",
                            pauseOnHover: true,
                            duration: 300
                        }
                    });
                }
                testimonialsInit();
                jQuery(window).resize(function() {
                    testimonialsInit();
                });
                var tControlsHeight = jQuery(".testimonials-controls").innerHeight();
                jQuery(".testimonials-controls").css("margin-top" , -tControlsHeight/2);
            });
        </script>
